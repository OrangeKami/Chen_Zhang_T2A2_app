<% if current_user %>
  <p>
  <%= button_to "Sign Out", destroy_user_session_path, :method => :delete %>
  </p>
  <p>
  <%= link_to "My Profile", edit_user_registration_path %>
  </p>
  <% if current_user.avatar.present? %>
    <%= image_tag(current_user.avatar.variant(resize_to_fill: [50, 50]), class: "rounded-circle") %>
  <% else %>
    <img alt="image" class="rounded-circle" src="/assets/default_avatar.jpg" width="50" height="50" >
  <% end %>
<% else %>
  <p>
  <%= link_to "Sing Up", new_user_registration_path %>
  </p>
  <p>
  <%= link_to "Sing In", new_user_session_path %>
  </p>
<% end %>



